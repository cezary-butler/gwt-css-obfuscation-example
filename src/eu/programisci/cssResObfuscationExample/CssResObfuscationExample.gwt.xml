<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="CssResObfuscationExample">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>

    <inherits name="com.google.gwt.resources.Resources" />
    <set-configuration-property name="CssResource.style" value="stable" />

    <!-- Specify the app entry point class.                   -->
    <entry-point class='eu.programisci.cssResObfuscationExample.client.CssResObfuscationExample'/>


    <!-- Specify the app servlets.                   -->
    <servlet path='/CssResObfuscationExampleService'
             class='eu.programisci.cssResObfuscationExample.server.CssResObfuscationExampleServiceImpl'/>

</module>